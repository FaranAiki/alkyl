; Module: main_module

; Globals
@str.1 = constant string "%lf"
@str.0 = constant string "a"

define func i32 @main() {
entry:
  %0 = alloca i32
  store 5, %0
  %1 = load %0
  %2 = cast %1
  %3 = cast 0
  %4 = eq %2, %3
  br.cond %4, then (else)
then:
  %5 = call @print (@str.0)
  br merge
else:
  %6 = load %0
  %7 = call @print (@str.1, %6)
  br merge
merge:
}
